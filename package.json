{
  "name": "sapient_react_training_project_gs",
  "version": "1.0.0",
  "description": "Sapient GS React Training Project",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "devNoServererver": "webpack --d --watch",
    "dev": "./node_modules/.bin/webpack-dev-server --content-base=src/frontend",
    "build": "./node_modules/.bin/webpack -d"
  },
  "jest": {
    "rootDir": "src/frontend/app",
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "author": "Hemanth",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "npm": "^5.7.1",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "webpack": "^4.0.1",
    "webpack-dev-server": "^3.1.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "file-loader": "^1.1.10",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "node-debug": "^0.1.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.5.1",
    "style-loader": "^0.20.2",
    "url-loader": "^0.6.2",
    "webpack-cli": "^2.0.9"
  }
